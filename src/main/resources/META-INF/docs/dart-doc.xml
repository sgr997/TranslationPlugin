<idea-plugin>
    <extensions defaultExtensionNs="cn.yiiguxing.plugin.translate">
        <documentationElementProvider
                language="Dart"
                implementationClass="cn.yiiguxing.plugin.translate.provider.DartDocumentationElementProvider"/>
    </extensions>

    <extensions defaultExtensionNs="com.intellij">
        <lang.documentationProvider
                id="Translation.TranslatedDocumentationProvider.Dart"
                order="first"
                language="Dart"
                implementationClass="cn.yiiguxing.plugin.translate.provider.TranslatedDocumentationProvider"/>
    </extensions>
</idea-plugin>